{
  "functions": {
    "app/api/v0-generate/route.ts": {
      "maxDuration": 30
    },
    "api/v0-python-pipeline.py": {
      "runtime": "python3.9",
      "maxDuration": 25
    }
  },
  "buildCommand": "pnpm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "env": {
    "V0_API_KEY": "@v0_api_key"
  }
}
